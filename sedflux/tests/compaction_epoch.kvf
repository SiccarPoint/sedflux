/* For the benchmark test, deposit sediment from a file.
   All other processes are off.  This is not intended to
   be a test of sedflux, only how quickly the architecture
   is working.
*/
[ bbl ]
active:                                 yes
logging:                                no
repeat interval:                        always
algorithm (none|muds):                  none
external sediment source file:		bench_input_be.seq

[ compaction ]
active:                                 yes
logging:                                no
repeat interval:                        10y

[ 'data dump' ]
active:                                 yes
logging:                                no
repeat interval:                        at-end
output directory:                       ./
vertical resolution:                    full
horizontal resolution:                  full
vertical limits:                        tight
horizontal limits:                      tight
property:                               grain

